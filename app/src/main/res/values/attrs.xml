<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="LoadingView">
        <attr name="lotti_rows" format="reference" />
        <attr name="error_text" format="reference|string" />
        <attr name="empty_text" format="reference|string" />
        <attr name="error_text_size" format="reference|dimension" />
        <attr name="state" format="enum">
            <enum name="loading" value="1"/>
            <enum name="error" value="2"/>
            <enum name="success" value="3"/>
            <enum name="empty" value="4"/>
        </attr>
    </declare-styleable>
</resources>